<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="185dp"
    android:layout_height="91dp">
    <ImageView
        android:id="@+id/rl_item_pheptoan_bg_one"
        android:background="@drawable/item_equal"
        android:layout_width="185dp"
        android:layout_height="91dp"/>

    <TextView
        android:id="@+id/tv_item_pheptoan_one"
        android:layout_centerInParent="true"
        android:textSize="25dp"
        android:textColor="@android:color/black"
        android:gravity="center"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/tv_item_pheptoan_one_flag"
        android:layout_centerInParent="true"
        android:gravity="center"
        android:textColor="@android:color/black"

        android:autoSizeTextType="uniform"
        android:textSize="65dp"
        android:autoSizeStepGranularity="2sp"
        android:ellipsize="none"

        android:layout_width="match_parent"
        android:layout_height="match_parent"/>

    <ImageView
        android:id="@+id/rl_item_bg_tranfom_one"
        android:background="@drawable/item_tranfom"
        android:layout_width="185dp"
        android:layout_height="91dp"/>


</RelativeLayout>